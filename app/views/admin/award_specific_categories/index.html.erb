<% provide(:title, 'Award Specific Categories') %>
<%= render 'breadcrumbs' %>
<h1>Award Specific Categories</h1>
<table class="table">
  <tr>
    <th>Nombre</th>
    <th></th>
  </tr>
  <% @award_specific_categories.each do |award_specific_category| %>
    <tr>
      <td><%= award_specific_category.name %></td>
      <td><%= render_index_actions [award_specific_category], only: [:edit, :destroy] %></td>
    </tr>
  <% end %>
</table>
<br/>
<%= link_to new_admin_award_award_specific_category_path, class: 'btn btn-default' do %>
  <i class="icon-plus"></i>
  Nuevo Award Specific Category
<% end %>
