<div class="row-fluid">
  <div class="span2">Nombre:</div>
  <div class="span10">
    <%= @contact_ticket.name %>
  </div>
</div>
<div class="row-fluid">
  <div class="span2">De:</div>
  <div class="span10">
    <%= @contact_ticket.from %>
  </div>
</div>
<div class="row-fluid">
  <div class="span2">Asunto:</div>
  <div class="span10">
    <%= @contact_ticket.subject %>
  </div>
</div>
<div class="row-fluid">
  <div class="span2">Contenido:</div>
  <div class="span10">
    <%= @contact_ticket.content %>
  </div>
</div>
<%= link_to admin_contact_tickets_path, class: 'btn btn-default' do %>
  <i class="icon-arrow-left"></i>
  Volver
<% end %>
