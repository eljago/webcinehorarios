<h1>
  PDT de
  <%= @function_type.name %>
</h1>
<table class="table">
  <tr>
    <th>Nombre</th>
    <th>Cine</th>
    <th></th>
  </tr>
  <% @parse_detector_types.each do |parse_detector_type| %>
    <tr>
      <td>
        <%= parse_detector_type.name %>
      </td>
      <td>
        <%= parse_detector_type.cinema.name %>
      </td>
      <td>
        <%= render_index_actions [@function_type, parse_detector_type] %>
      </td>
    </tr>
  <% end %>
</table>
<br/>
<%= link_to new_admin_function_type_parse_detector_type_path(@function_type), class: 'btn btn-default' do %>
  <i class="icon-plus"></i>
  Nuevo PDT
<% end %>
<%= link_to admin_function_types_path, class: 'btn btn-default' do %>
  <i class="icon-arrow-left"></i>
  Volver
<% end %>
