<h1>
  PDT de 
  <%= @function_type.name %>
</h1>
<div class="row-fluid">
  <div class="span2">Nombre:</div>
  <div class="span3">
    <%= @parse_detector_type.name %>
  </div>
</div>
<div class="row-fluid">
  <div class="span2">Cinema:</div>
  <div class="span3">
    <%= @parse_detector_type.cinema.name %>
  </div>
</div>
<%= link_to [:admin, @function_type, :parse_detector_types], class: 'btn btn-default' do %>
  <i class="icon-arrow-left"></i>
  Volver
<% end %>
<%= link_to [:edit, :admin, @function_type, @parse_detector_type], class: 'btn btn-default' do %>
  <i class="icon-edit"></i>
  Editar
<% end %>
